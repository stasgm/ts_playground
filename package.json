{
  "name": "ts_starter",
  "version": "1.0.0",
  "main": "closure.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc --build",
    "start": "ts-node ./src/index.ts",
    "dev": "tsnd --respawn src/index.ts",
    "dev2": "tsnd --respawn src/closure.ts",
    "lint": "tsc --noEmit && eslint \"src/**/*.ts\" --quiet --fix",
    "test": "jest"
  },
  "dependencies": {
    "ts-node": "^10.2.1",
    "ts-node-dev": "^1.1.8",
    "supertest": "6.1.6",
    "express": "4.17.1"
  },
  "devDependencies": {
    "@types/eslint": "^7.28.0",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.10.3",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "@types/express": "4.17.13",
    "@types/supertest": "2.0.11",
    "jest": "^27.2.4",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.3"
  }
}
